{
    "name": "garak/autocompleter-sandbox",
    "license": "LGPL",
    "type": "project",
    "description": "A sandbox for PUGXAutocompleterBundle",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "garakkio@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "App\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "require": {
        "php": "^7.1",
        "doctrine/doctrine-bundle": "^1.8",
        "doctrine/orm": "^2.5",
        "pugx/autocompleter-bundle": "^1.2",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/asset": "^3.4",
        "symfony/console": "^3.4",
        "symfony/flex": "^1.0",
        "symfony/form": "^3.4",
        "symfony/framework-bundle": "^3.4",
        "symfony/lts": "3",
        "symfony/monolog-bundle": "^3.1",
        "symfony/security-csrf": "^3.4",
        "symfony/templating": "^3.4",
        "symfony/twig-bundle": "^3.4",
        "symfony/validator": "^3.4",
        "symfony/yaml": "^3.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.8",
        "phpunit/phpunit": "^7.0",
        "symfony/browser-kit": "^3.4",
        "symfony/css-selector": "^3.4",
        "symfony/debug-bundle": "^3.4",
        "symfony/dotenv": "^3.4",
        "symfony/phpunit-bridge": "^3.4",
        "symfony/profiler-pack": "^1.0",
        "symfony/web-server-bundle": "^3.4",
        "willdurand/faker-bundle": "^1.3"

    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "symfony-assets-install": "relative"
    }
}
